<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextField fx:id="categorieTextfield" layoutX="38.0" layoutY="54.0" promptText="Catégorie"/>
        <TextArea fx:id="descriptionCategorieTextArea" layoutX="24.0" layoutY="115.0" prefHeight="96.0"
                  prefWidth="200.0" promptText="Description Catégorie"/>
        <RadioButton fx:id="descendante" layoutX="311.0" layoutY="106.0" mnemonicParsing="false"
                     text="Vente descendante"/>
        <RadioButton fx:id="dureeLimitee" layoutX="311.0" layoutY="154.0" mnemonicParsing="false" text="Durée limitée"/>
        <RadioButton fx:id="encheresNonLibres" layoutX="311.0" layoutY="67.0" mnemonicParsing="false"
                     text="Enchères non libres"/>
        <RadioButton fx:id="revocable" layoutX="311.0" layoutY="191.0" mnemonicParsing="false" text="Vente révocable"/>
        <DatePicker fx:id="datePicker" layoutX="300.0" layoutY="251.0"/>
        <Button layoutX="224.0" layoutY="336.0" mnemonicParsing="false" onAction="#onAjoutSalle" text="Créer Salle"/>
        <TextField fx:id="heureTextfield" layoutX="301.0" layoutY="290.0" promptText="Heure ex : 11:54"/>
    </children>
</AnchorPane>
